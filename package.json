{
  "name": "ocac-formations",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Simon Renoult <simon.renoult@octo.Com>",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "dependencies": {
    "@types/luxon": "^2.0.7",
    "luxon": "^2.1.1",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.23",
    "@types/node": "12.12.8",
    "babel-core": "6.26.3",
    "babel-jest": "24.9.0",
    "jest": "24.9.0",
    "ts-jest": "24.1.0",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "type-fest": "^2.8.0",
    "typescript": "3.7.2"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/test"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(\\.|/)(test)\\.(js|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  },
  "standard": {
    "env": [
      "node",
      "jest"
    ],
    "globals": [
      "expect"
    ]
  }
}
